# game/script/ending.rpy:656
translate German ending_lpd_bad1_f6b1d5c1:

    # b2 "Sometimes, I just feel a little lonely, nothing more or nothing less.{nw}"
    b2 "Manchmal fühle ich mich ein klein wenig Einsam. Nicht mehr, nicht weniger.{nw}"

# game/script/ending.rpy:660
