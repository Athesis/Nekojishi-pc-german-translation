# game/script/ending.rpy:1300
translate German cg_lpdcat_end_d558cb57:

    # lpdcat_true normal "\"I didn't either.\" "
    # lpdcat_true normal "\"I thought of so many ways I could try to run away, all the ways I could have been caught, all the ways I could have died.\" "
    # lpdcat_true normal "\"But what happened was something I could never have thought of.\" "
    lpdcat_true normal "\"Das habe ich auch nicht.\" "
    lpdcat_true normal "\"Ich habe mir die Möglichkeiten vorgestellt wie ich entkommen könnte, wie man mich fangen könnte, wie ich am Ende hätte sterben können.\" "
    lpdcat_true normal "\"Aber was passiert ist, ist etwas, dass ich mir nicht vorstellen konnte.\" "

# game/script/ending.rpy:1302
translate German cg_lpdcat_end_73dba62e:

    # lpdcat_true serious "\"I can't say that I'm happy about it...\" "
    # lpdcat_true serious "\"It's... {w=0.5}a complicated feeling.\" "
    lpdcat_true serious "\"Ich kann nicht sagen das ich fröhlich darüber bin...\" "
    lpdcat_true serious "\"Es ist... {w=0.5}ein kompliziertes Gefühl.\" "

# game/script/ending.rpy:1308
translate German cg_lpdcat_end_5e9df0a7:

    # xiunuosi eyes_close "\"Surely? You seem happy to me.\" "
    xiunuosi eyes_close "\"Sicher? Du scheinst mir fröhlich.\" "

