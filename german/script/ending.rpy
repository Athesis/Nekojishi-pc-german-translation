translate German cg_lpdcat_end_6fb0aa6a:

    # lpdcat_true difficult "\"For what? Please don't...\" "
    # lpdcat_true difficult "\"For my sworn enemy to come and congratulate me... it's weird...\" "
    lpdcat_true difficult "\"Für was? Bitte lass das...\" "
    lpdcat_true difficult "\"Das mein eingeschworener Feind kommt und mir gratuliert ist... seltsam...\" "

# game/script/ending.rpy:1335
translate German cg_lpdcat_end_75a4b061:

    # xiunuosi eyes_close "\"My feelings are just as complicated as yours. After all, a yaoguai I've hunted for many years has suddenly become a god, how should I react?\" "
    xiunuosi eyes_close "\"Meine Gefühle sind genauso kompliziert wie deine. Schließlich ist ein Yaoguai den ich seid Jahren jage plötzlich zu einem Gott geworden. Wie soll ich mich da verhalten?\" "

# game/script/ending.rpy:1339
translate German cg_lpdcat_end_4d05f02c:

    # lpdcat_true cute "\"Hmm... {w=0.5}congratulate me?\" "
    lpdcat_true cute "\"Hmm... {w=0.5}mir gratulieren?\" "

# game/script/ending.rpy:1341
translate German cg_lpdcat_end_f7938736:

